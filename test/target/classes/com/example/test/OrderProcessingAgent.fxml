<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test.Agent">
   <children>
      <TableView fx:id="tableView" layoutX="38.0" layoutY="28.0" onMouseClicked="#tableViewMouseClicked" prefHeight="225.0" prefWidth="525.0">
         <columns>
            <TableColumn fx:id="type" prefWidth="100.0" text="Pizza Type" />
            <TableColumn fx:id="toopings" minWidth="5.0" prefWidth="212.0" text="Toppings" />
            <TableColumn fx:id="pickupTime" prefWidth="109.0" text="Pickup Time" />
            <TableColumn fx:id="status" prefWidth="103.0" text="Status" />
         </columns>
      </TableView>
      <Button layoutX="191.0" layoutY="335.0" mnemonicParsing="false" onAction="#btnMainMenuAction" prefHeight="31.0" prefWidth="220.0" text="Home Screen">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
      <Button layoutX="191.0" layoutY="278.0" mnemonicParsing="false" onAction="#btnAcceptedAction" prefHeight="31.0" prefWidth="220.0" text="Ready for Preparation">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
